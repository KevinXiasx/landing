<!DOCTYPE html>
<html>
<head>
	<title>Radxa project</title>
	<link rel="stylesheet" type="text/css" href="http://172.168.1.88:3000/stylesheets/bootstrap/css/bootstrap.min.css">
	<link rel="stylesheet" href="/stylesheets/Font-Awesome/css/font-awesome.min.css">
	
	<link rel="stylesheet" type="text/css" href="/stylesheets/homepage/font.css">
	<link rel="stylesheet" type="text/less" href="/less/project.less">
	<meta http-equiv="X-UA-Compatible" content="IE=10" />
</head>

<body data-spy="scroll" data-target=".hehee">
	<% include ../header %>
	<div class="jumbotron marked">
		<div class="container" id="bigbord">
			<%- index %>
	  	</div>
	</div>
	<div class="allcontent container">

		<div class="row">
			<div class="rightframe col-md-2 visible-md visible-lg">
 				<div class='bs-sidebar hehee ' role="complementary">
					<ul class="nav nav-pills nav-stacked" data-spy="affix" data-offset-top="500">
            		</ul>
				</div>
			</div>
			<div class="centerframe col-md-9 col-xs-12 marked">
				<% for( var s = 0; s < marks.length; s++ ){ %>
					<section class="sec">
					<div class="marktext">
						<%- marks[s].text %>
					</div>
					<div class="imgswrap">
						<% for( var k = 0; k < (marks[s].imgs).length; k++ ){ %>	
							<a href="#portfolioModal1" class="portfolio-link" data-toggle="modal">
								<%- (marks[s].imgs)[k] %>
							</a>
						<% } %>
					</div>
					</section> 
				<% } %>
			</div>
			<div class="leftframe col-md-1 visible-md visible-lg">
			<a href="#header">
				<div class="gototop flexbox direc-col " data-spy="affix" data-offset-top="500">
					<i class="fa fa-hand-o-up"></i>
					<span>Top</span>
				</div>
			</a>
			</div>
		</div>
	</div>

	<div class="portfolio-modal modal fade" id="portfolioModal1" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-content">
            <div class="container">
                <div class="modal-body flexbox direc-col">
                	<button type="button" class="btn btn-default" data-dismiss="modal"><i class="fa fa-times"></i> Close</button>
                    <img src="" class="img-responsive img-centered" data-dismiss="modal" alt="">
                </div>
            </div>
        </div>
    </div>
	<% include ../bottom %>

	<script src="http://172.168.1.88:3000/javascripts/seajs-3.0.0/dist/sea.js"></script>

	<script>
	  seajs.config({
	    base: "/javascripts/",
	    alias: {
	    	"jquery": "http://172.168.1.88:3000/javascripts/jquery/jquery-seajs.js",
	    	"bootstrap-js": "http://172.168.1.88:3000/stylesheets/bootstrap/js/bootstrap-seajs.js",
	    	"less": "http://172.168.1.88:3000/javascripts/less.js"
	    }
	  });

	   seajs.use("project/main-project");
	</script>

</body>
</html>