<!DOCTYPE html>
<html>
<head>
	<title>Radxa project</title>
	<link rel="stylesheet" type="text/css" href="http://172.168.1.88:3000/stylesheets/bootstrap/css/bootstrap.min.css">
	<link rel="stylesheet" href="/stylesheets/Font-Awesome/css/font-awesome.min.css">

	<script type="text/javascript" src="http://172.168.1.88:3000/javascripts/jquery/jquery-1.11.2.js"></script>
	<script type="text/javascript" src="http://172.168.1.88:3000/javascripts/jquery/jq-mousewheel.js"></script>
	<script type="text/javascript" src="http://172.168.1.88:3000/javascripts/jquery/jq-transition.js"></script>
	<script type="text/javascript" src="http://172.168.1.88:3000/stylesheets/bootstrap/js/bootstrap.min.js"></script>
	
	<link rel="stylesheet" type="text/css" href="/stylesheets/homepage/font.css">
	<link rel="stylesheet" type="text/less" href="/less/more.less">
</head>

<body >
	<% include ../header %>
	<div class="jumbotron marked">
		<div class="back"></div>
		<div class="container" id="bigbord">
	  		<%- index %>
	  	</div>
	</div>
	<div class="content marked">
		<% for( var s = 0; s < marks.length; s++ ){ %>
			<% if( s%2 == 0){ %>
			<div class="boxborder " style="background-color:#56BC8A ; ">
			<div class="container">
				<div class="row flexbox " >
					<div class="col-xs-7">
						 <%- marks[s].text %>
					</div>
					<div class="col-xs-5">
						<%- marks[s].img %>
					</div>
				</div>
				</div>
			</div>
			<% } else { %>
			<div class="boxborder" style="background-color:#0199cb">
			<div class="container">
				<div class="row flexbox ">					
					<div class="col-xs-5">
						<%- marks[s].img %>
					</div>
					<div class="col-xs-7">
						 <%- marks[s].text %>
					</div>
				</div>
				</div>
			</div>
			<% } }%>
	</div>
	</div>
	<% include ../bottom %>

	<script type="text/javascript">
		loadl('more');
	</script>

	<script type="text/javascript" src="http://172.168.1.88:3000/javascripts/less.js">
	</script>

</body>
</html>